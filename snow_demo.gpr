with "config/snow_config.gpr";
with "snow.gpr";

project Snow_Demo is

   for Source_Dirs use ("src/");
   for Object_Dir use "obj/" & Snow_Config.Build_Profile;
   for Create_Missing_Dirs use "True";
   for Exec_Dir use "bin";
   for Main use ("snow_demo.adb");

   package Compiler is
      for Default_Switches ("Ada") use Snow_Config.Ada_Compiler_Switches;
   end Compiler;

   package Binder is
      for Switches ("Ada") use ("-Es"); --  Symbolic traceback
   end Binder;

end Snow_Demo;
